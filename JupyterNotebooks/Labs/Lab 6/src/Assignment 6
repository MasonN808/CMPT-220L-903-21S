class ListItem{
    static int value;
    static ListItem newItem;

    public ListItem(int value){
        this(null);
        this.value = value;
    }
    public ListItem(ListItem newItem){
    }




    public static void main(String args []){
        ListItem item_1 = new ListItem(1);
        ListItem item_2 = new ListItem(item_1);
        ListItem item_3 = new ListItem(item_2);
        ListItem item_4 = new ListItem(item_3);
        ListItem item_comb = new ListItem(item_4);

    }
}
